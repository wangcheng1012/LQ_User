<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              style="@style/ll_details_content"
              android:id="@+id/ll_friend_friends_item"
              android:layout_marginBottom="0dp"
              android:layout_marginTop="15dp"
              android:padding="10dp">

    <ImageView
        android:id="@+id/iv_friends_headimg"
        android:layout_width="35dp"
        android:layout_height="35dp"
        />

    <TextView
        android:id="@+id/tv_friends_nickname"
        style="@style/ww.black"
        android:layout_marginLeft="15dp"
        android:text="昵称"
        />

    <TextView
        android:id="@+id/tv_friends_age"
        style="@style/ww.white"
        android:layout_marginLeft="15dp"
        android:background="@drawable/bg_theme2_solid_ellipse"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="♂ 00"
        android:textSize="12sp"
        />

    <ImageView
        android:id="@+id/iv_team_flag"
        style="@style/ww"
        android:visibility="gone"
        android:layout_marginLeft="15dp"
        android:src="@drawable/team_flag"
        />


</LinearLayout>